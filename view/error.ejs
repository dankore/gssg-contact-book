<!-- HEADER -->
<%- include('includes/header') %>
<!-- HEADER END-->
<div class="flex min-h-full flex-col pt-16 pb-12">
  <main class="mx-auto flex w-full max-w-7xl bg-white flex-grow flex-col justify-center px-4 sm:px-6 lg:px-8">
    <div class="py-16">
      <div class="text-center">
        <h1 class="mt-2 mb-5 text-4xl font-bold tracking-tight text-gray-900 sm:text-5xl">An error was found:</h1>

        <p class="text-red-500"><%= status %>: <%= errorMsg %></p>
        <div class="mt-6">
          <a href="/" class="text-base font-medium text-primary-700 hover:text-primary-500">
            Go back home
            <span aria-hidden="true"> &rarr;</span>
          </a>
        </div>
      </div>
    </div>
  </main>
  <!-- FOOTER-->
  <%- include('includes/footer') %>
  <!-- FOOTER END-->
</div>
